<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/nav_main">
    <fragment
        android:id="@+id/nav_main"
        android:name="com.example.fullyminded.Fragments.MainFragment"
        android:label="Fully Minded"
        tools:layout="@layout/fragment_main" />
    <fragment
        android:id="@+id/nav_difference"
        android:name="com.example.fullyminded.Fragments.DefinitionFragment"
        android:label="Descriptions"
        tools:layout="@layout/fragment_definition" />
    <fragment
        android:id="@+id/nav_profile"
        android:name="com.example.fullyminded.Fragments.ProfileFragment"
        android:label="Profile"
        tools:layout="@layout/fragment_profile" />
    <fragment
        android:id="@+id/nav_sign"
        android:name="com.example.fullyminded.Fragments.SignUpFragment"
        android:label="Sign Up"
        tools:layout="@layout/fragment_sign_up" />
    <fragment
        android:id="@+id/nav_check"
        android:name="com.example.fullyminded.Fragments.CheckInFragment"
        android:label="fragment_check_in"
        tools:layout="@layout/fragment_check_in" />
    <fragment
        android:id="@+id/nav_counselor"
        android:name="com.example.fullyminded.ListView.CounselorsFragment"
        android:label="Counselors"
        tools:layout="@layout/fragment_counselors" >
        <action
            android:id="@+id/action_nav_counselor_to_secondListPageFragment"
            app:destination="@id/nav_second" />
    </fragment>
    <fragment
        android:id="@+id/nav_second"
        android:name="com.example.fullyminded.ListView.SecondListPageFragment"
        android:label="Counselor"
        tools:layout="@layout/fragment_second_list_page" />
</navigation>